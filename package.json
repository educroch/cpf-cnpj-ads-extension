{
	"name": "cpfcnpjextension",
	"displayName": "CPF / CNPJ Extension",
	"description": "Formatação e remoção de formatação de CPF e CNPJ.",
	"publisher": "Eduardo Cogo da Rocha",
	"version": "1.0.0",
	"engines": {
		"vscode": "^1.39.0",
		"azdata": "*"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:cpfcnpjextension.removerFormatacaoCpf"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "cpfcnpjextension.removerFormatacao",
				"title": "Remover Formatação do Documento."
			},{
				"command": "cpfcnpjextension.formatarDocumento",
				"title": "Formatar Documento."
			}
		],
		"keybindings": [
			{
				"command": "cpfcnpjextension.removerFormatacao",
				"key": "f3"
			},
			{
				"command": "cpfcnpjextension.formatarDocumento",
				"key": "Alt+f3"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile && yarn run lint",
		"test": "node ./out/test/runTest.js",
		"proposedapi": "node installTypings.js"
	},
	"devDependencies": {
		"@types/azdata": "*",
		"@types/glob": "^7.1.4",
		"@types/mocha": "^9.0.0",
		"@types/node": "^13.11.0",
		"@types/vscode": "^1.39.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"@typescript-eslint/parser": "^2.30.0",
		"azdata-test": "^1.x",
		"eslint": "^6.8.0",
		"glob": "^7.1.7",
		"mocha": "^9.1.0",
		"typescript": "^4.3.5"
	},
	"dependencies": {
		"cpf-cnpj-validator": "^1.0.3"
	}
}
